--APAGA A TABELA TEMPORÁRIA, CASO EXISTA
DROP TABLE IF EXISTS #TB_VENDA
GO

--CRIA A TABELA VIRTUAL --> # ANTES DO NOME
CREATE TABLE #TB_VENDA (
  ID_VENDA INT IDENTITY (1,1)
  DAT_VENDA DATE,
  VAL_VENDA MONEY
)

--INSERE DADOS NA TABELA VIRTUAL --> NÃO ESQUEÇA DA # 
INSERT INTO #TB_VENDA VALUES ('12/03/2022',1380)


SELECT * FROM #TB_VENDA
